{
    "manifest_version": 3,
    "name": "Web Traffic Anotator - Browser extension",
    "description": "Provides metadata about requests and responses to the Web Traffic Annotator, that pairs these data with network flow identification.",
    "version": "1.0.0",
    "key": "",

    "permissions": [
      "nativeMessaging",
      "webRequest"
    ],
    "host_permissions": [
        "<all_urls>"
    ],

    "action": {},

    "background": {
      "service_worker": "serviceWorker.js"
    },

    "icons": {
      "32": "Icons/pen.png"
    }
  }